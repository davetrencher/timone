<idea-plugin url="https://github.com/trampusmaximus">
	
	<name>TunnelliJ</name>
	
	<id>TunnelliJ</id>	

	<description>
		TunnelliJ plugin works as a TCP tunnel and it useful for any network development.
		It allows you to monitor, sniff TCP connections and display the content in a very 
		effective way inside your IDE.
	</description>
	
	<version>2.0</version>
	
	<vendor url="https://github.com/trampusmaximus">Dave Trencher</vendor>

	<idea-version since-build="145.258"/>
	
	<change-notes>repackaged for Idea 16.x</change-notes>
	
	<category>network</category>
	
	<project-components>
		<component>
			<implementation-class>net.boruvka.idea.tunnellij.TunnelPlugin</implementation-class>
			<interface-class>net.boruvka.idea.tunnellij.TunnelPlugin</interface-class>
		</component>
	</project-components>
	
</idea-plugin>
